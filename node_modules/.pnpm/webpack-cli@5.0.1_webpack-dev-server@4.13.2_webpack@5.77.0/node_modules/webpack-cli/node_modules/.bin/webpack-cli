#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/z/code/z/z1/mct/node_modules/.pnpm/webpack-cli@5.0.1_webpack-dev-server@4.13.2_webpack@5.77.0/node_modules/webpack-cli/bin/node_modules:/home/z/code/z/z1/mct/node_modules/.pnpm/webpack-cli@5.0.1_webpack-dev-server@4.13.2_webpack@5.77.0/node_modules/webpack-cli/node_modules:/home/z/code/z/z1/mct/node_modules/.pnpm/webpack-cli@5.0.1_webpack-dev-server@4.13.2_webpack@5.77.0/node_modules:/home/z/code/z/z1/mct/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/z/code/z/z1/mct/node_modules/.pnpm/webpack-cli@5.0.1_webpack-dev-server@4.13.2_webpack@5.77.0/node_modules/webpack-cli/bin/node_modules:/home/z/code/z/z1/mct/node_modules/.pnpm/webpack-cli@5.0.1_webpack-dev-server@4.13.2_webpack@5.77.0/node_modules/webpack-cli/node_modules:/home/z/code/z/z1/mct/node_modules/.pnpm/webpack-cli@5.0.1_webpack-dev-server@4.13.2_webpack@5.77.0/node_modules:/home/z/code/z/z1/mct/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
